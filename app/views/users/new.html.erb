<%= form_for @user, url: {action: :create} do |f| %>
  <%= f.label :uid, 'uid' %> 
  <%= f.text_field :uid %> 
  <p>
  <%= f.label :password, 'パスワード' %>
  <%= f.password_field :password %>
  </p>
  <p>
  <%= f.label :password_confirmation, 'パスワード(確認)' %>
  <%= f.password_field :password_confirmation %>
  </p> 
  <%= f.submit '登録' %>
<% end %>


<%= link_to "戻る", :back %>


<% flash.each do |key,value| %>
  <p><%= value %><p>
<% end %>